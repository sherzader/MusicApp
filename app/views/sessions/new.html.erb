<h1>Log In</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label for="user_email">Email:</label>
    <input
      type="text"
      name="user[email]"
      id="user_email">
  <br>
  <label for="user_password">Password:</label>
  <input type="password" name="user[password]" id="user_password">
  <br>
  <input type="submit" value="Log In">
</form>
