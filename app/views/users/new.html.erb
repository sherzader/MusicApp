<!DOCTYPE html>
<head>
  <h1>Sign up!</h1>
</head>
<body>
    <th>Enter your information:</th>
  <form action="<%= users_url %>" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <table>
    <tr>
      <th><label for="user_email">Email:</label></th>
      <th>
        <input type="text"
          name="user[email]"
          id="user_email">
          <!-- value="<%= @user.email %>"> -->
        </input>
      </th>
    </tr>
    <tr>
      <th><label for="user_password">Password:</label></th>
      <th>
        <input type="text"
          name="user[password]"
          id="user_password">
          <!-- value="<%= @user.password %>"> -->
        </input>
      </th>
    </tr>
  </table>
  <input type="submit" value="Create user">
  </form>
</body>
</html>
