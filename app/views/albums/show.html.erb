<h1><%= @album.name %></h1>
<%= auth_token %>
<% @album.tracks.each do |track| %>
  <ul>
  <li><%= track.song %></li>
  <%= button_to "Remove", track_url(track.id), method: :delete %>
  </ul>
  <% end %>
  <br>
<%= button_to "Add Tracks", new_album_track_url(@album), method: :get %>
<br>
<%= link_to "Edit Album", edit_album_url(@album) %>
<%= link_to "Delete Album", album_url(@album), method: :delete %>
<br>
<p>More: <%= link_to @album.band.name, band_url(@album.band) %></p>
